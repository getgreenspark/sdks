{
  "name": "@getgreenspark/widgets",
  "version": "2.2.0-1",
  "bugs": {
    "url": "https://github.com/getgreenspark/sdks/issues"
  },
  "main": "./dist/widgets@latest.js",
  "description": "An SDK design to help the use of Greenspark's widget API in the browser",
  "author": "Greenspark <support@getgreenspark.com>",
  "keywords": [
    "greenspark",
    "getgreenspark",
    "estimation",
    "estimations",
    "api",
    "carbon offset",
    "carbon offsetting",
    "carbon footprint",
    "carbon estimation",
    "carbon removal",
    "trees",
    "plastic",
    "plant trees",
    "offset plastic",
    "carbon neutral",
    "sustainability",
    "carbon emissions"
  ],
  "scripts": {
    "test": "jest --config=jest.config.ts",
    "lint": "eslint src",
    "dev": "webpack --mode=development",
    "build": "rm -r dist; mkdir dist; webpack --mode=production",
    "release": "release-it",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "homepage": "https://github.com/getgreenspark/sdks",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/getgreenspark/sdks.git"
  },
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-links": "^8.6.9",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.9",
    "@storybook/html": "^8.6.9",
    "@storybook/html-webpack5": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^12.11.5",
    "@types/webpack-env": "^1.18.5",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.9.1",
    "eslint-plugin-storybook": "^0.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "release-it": "^17.6.0",
    "storybook": "^8.6.9",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "webpack": "5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "tag": true,
      "commitMessage": "@getgreenspark/widgets v${version}",
      "tagName": "widgets-v${version}",
      "tagAnnotation": "@getgreenspark/widgets v${version}"
    },
    "npm": {
      "publish": true,
      "publishArgs": [
        "--include-workspace-root=false"
      ]
    },
    "hooks": {
      "before:init": [
        "npm run lint",
        "npm run test",
        "npm run build"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "directories": {
    "src": "src"
  }
}
